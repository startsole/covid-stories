{
  "name": "thefront",
  "author": "maccarian agency",
  "email": "hi@maccarianagency.com",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write '**/*.{js,css,html}'",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "assets": "rimraf ./public/images && cp-cli ./node_modules/thefront-assets/images ./public/images",
    "postinstall": "npm run assets"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.39",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.2.1",
    "aos": "^2.3.4",
    "clsx": "^1.0.4",
    "history": "^4.9.0",
    "jarallax": "^1.12.1",
    "leaflet": "^1.6.0",
    "node-sass": "^4.14.1",
    "npm": "^6.14.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-cookie-consent": "^3.0.0",
    "react-countup": "^4.3.3",
    "react-dom": "^16.8.6",
    "react-lazy-load-image-component": "^1.5.0",
    "react-leaflet": "^2.7.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.4.1",
    "react-typed": "^1.2.0",
    "react-visibility-sensor": "^5.1.1",
    "start": "^5.1.0",
    "swiper": "^5.4.1",
    "thefront-assets": "git+https://github.com/gevorgmakaryan/thefront-assets.git#1.0.7",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "cp-cli": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.19.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-docgen": "^5.3.0",
    "react-styleguidist": "^11.0.8",
    "rimraf": "^3.0.2",
    "typescript": "^3.5.1"
